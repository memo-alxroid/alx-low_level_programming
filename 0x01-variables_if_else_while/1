
#include <stdio.h>

/**
 * main - print the size of various types
 *
 * Description: all possible combinations of two two-digit numbers
 * starting from 0
 *
 *
 * Return: Allways 0 (Success)
 *
 */

int main(void)
{
	int firstDigit = 0;
	int secondDigit;
	int thirdDigit;
	int fourthDigit;

	while (firstDigit < 10)
	{
		secondDigit = 0;
		while (secondDigit < 10)
		{
			thirdDigit = firstDigit;
			fourthDigit = secondDigit + 1;
			while (thirdDigit < 10)
			{
				while (fourthDigit < 10)
				{
					putchar(48 + firstDigit);
					putchar(48 + secondDigit);
					putchar(' ');
					putchar(48 + thirdDigit);
					putchar(48 + fourthDigit);
					if (firstDigit < 9 || secondDigit < 8 || thirdDigit < 9 || fourthDigit < 9)
					{
						putchar(',');
						putchar(' ');
					}
					fourthDigit++;
				}
				fourthDigit = 0;
				thirdDigit++;
			}
			secondDigit++;
		}
		firstDigit++;
	}
	putchar('\n');
	return (0);
}
